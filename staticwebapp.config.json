{
  "routes": [
    {
     "route": "/*",
      "statusCode":503
    }
    ],
      "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/static/js/*", "/static/css/*", "*.json"]
    },
      "mimeTypes": {
        ".json": "text/json",
        ".js": "text/javascript",
        ".css": "text/css"
    }
}
